	mcu1_r5f_cluster: mcu_r5f_cluster@5c00000 {
		compatible = "ti,dra822-mcu-r5f";
		lockstep-mode = <1>;
		#address-cells = <1>;
		#size-cells = <1>;
		ranges = <0x5c00000 0x00 0x5c00000 0x20000>,
			 <0x5d00000 0x00 0x5d00000 0x20000>;
		/*power-domains = <&k3_pds 129>;*/

		mcu1_r5f0: r5f@5c00000 {
			compatible = "ti,dra822-r5f";
			reg = <0x5c00000 0x00008000>,
			      <0x5c10000 0x00008000>;
			reg-names = "atcm", "btcm";
			/*ti,sci = <&dmsc>;*/
			/*ti,sci-dev-id = <159>;*/
			ti,sci-proc-ids = <0x01 0xFF>;
			/*resets = <&k3_reset 159 1>;*/
			atcm-enable = <1>;
			btcm-enable = <1>;
			loczrama = <1>;
			mboxes = <&mailbox0_cluster1 &mbox_mcu1_r5f0>;
		};

		mcu1_r5f1: r5f@5d00000 {
			compatible = "ti,dra822-r5f";
			reg = <0x5d00000 0x00008000>,
			      <0x5d10000 0x00008000>;
			reg-names = "atcm", "btcm";
			/*ti,sci = <&dmsc>;*/
			/*ti,sci-dev-id = <245>;*/
			ti,sci-proc-ids = <0x02 0xFF>;
			/*resets = <&k3_reset 245 1>;*/
