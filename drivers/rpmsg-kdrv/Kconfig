menu "Rpmsg virtual device drivers"

# RPMSG always gets selected by whoever wants it
config RPMSG_KDRV
	tristate "RPMSG virtual device interface"
	select RPMSG

config RPMSG_KDRV_DISPLAY
	tristate "RPMSG virtual display device support"
	select RPMSG_KDRV

endmenu
